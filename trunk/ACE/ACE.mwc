// $Id$
workspace {
  exclude(automake) {
    build
    Kokyu
  }
  exclude {
    TAO
  }

  // The associate scope does not add directories to the workspace;
  // it only associates a name with one or more directories.
  associate(examples) {
    examples
  }
  associate(tests) {
    performance-tests
    tests
  }
}
