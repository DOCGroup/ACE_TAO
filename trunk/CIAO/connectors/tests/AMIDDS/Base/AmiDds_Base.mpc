    AmiDds_BaseEC.cpp
    AmiDds_BaseAEC.cpp
  }

  Header_Files {
    AmiDds_BaseEC.h
    AmiDds_Base_lem_stub_export.h
}

  Inline_Files {
    AmiDds_BaseEC.inl
  }
}

