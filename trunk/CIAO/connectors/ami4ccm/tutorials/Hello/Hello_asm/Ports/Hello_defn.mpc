// $Id$

project(Hello_defn_idl_gen) : taoidldefaults, typecode {
  custom_only = 1
  idlflags += -Wb,stub_export_macro=HELLO_DEFN_STUB_Export \
              -Wb,stub_export_include=Hello_defn_stub_export.h \
              -SS -Gxhst \
              -o ../GeneratedCode

  IDL_Files {
    Hello_defn.idl
    gendir=../GeneratedCode
  }
}

project(Hello_defn_stub) : ccm_stub, ami, ami4ccm_stub {
  after += Hello_defn_idl_gen
  libpaths += ../lib
  libout = ../lib
  includes += ../GeneratedCode
  sharedname = Hello_defn_stub
  dynamicflags = HELLO_DEFN_STUB_BUILD_DLL

  IDL_Files {
  }

  Source_Files {
    ../GeneratedCode/Hello_defnC.cpp
  }

  Header_Files {
    ../GeneratedCode/Hello_defnC.h
    ../GeneratedCode/Hello_defn_stub_export.h
  }

}
