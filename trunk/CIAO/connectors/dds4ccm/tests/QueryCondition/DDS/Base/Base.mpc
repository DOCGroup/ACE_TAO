// $Id$

project(Base_stub) : taoidldefaults, dds4ccm_ts_default {
  sharedname = Base_stub
  dynamicflags += BASE_STUB_BUILD_DLL
  libout = ../descriptors

  idlflags += -SS -Sci \
              -Wb,stub_export_macro=BASE_STUB_Export \
              -Wb,stub_export_include=Base_stub_export.h

  IDL_Files {
    idlflags += -Gxhst
    Base.idl
  }

  opendds_ts_flags += --export=QUERY_CONDITION_TEST_BASE_STUB_Export

  DDSGenerator_Files {
    Base.idl
  }

  Source_Files {
  }
}

