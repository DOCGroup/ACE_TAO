  dynamicflags += NOMODULE_COMMON_STUB_BUILD_DLL
  libout = ../lib

  idlflags += -SS -Gxhst -Sci \
              -Wb,stub_export_macro=NOMODULE_COMMON_STUB_Export \
              -Wb,stub_export_include=NoModule_Common_stub_export.h -Glem

