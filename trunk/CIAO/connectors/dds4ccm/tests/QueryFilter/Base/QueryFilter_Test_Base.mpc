  dynamicflags += QUERY_FILTER_TEST_BASE_STUB_BUILD_DLL
  libout = ../lib

  idlflags += -SS -Gxhst -Sci \
              -Wb,stub_export_macro=QUERY_FILTER_TEST_BASE_STUB_Export \
              -Wb,stub_export_include=QueryFilter_Test_Base_stub_export.h

