// -*- MPC -*-
// $Id$

project : dds4ccm {
  Define_Custom(DDS4CCMTypeSupport) {
    automatic_in          = 0
    output_follows_input  = 0
    libpath               = $(ACE_ROOT)/lib
    command   = $(TAO_IDL)
    dependent = $(TAO_IDL_DEP)
    keyword idl_flags = commandflags
    commandflags += -Gts
    inputext              = TypeSupport.idl
    generic_pre_extension = TypeSupport
    generic_outputext     = .idl
  }
}

