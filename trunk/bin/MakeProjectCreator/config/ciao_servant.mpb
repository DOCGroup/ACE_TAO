// -*- MPC -*-
// $Id$

project : ciao_component {
  after += CIAO_Server
  libs  += CIAO_Server

  Define_Custom(CIDL) {
    automatic         = 0
    command           = $(CIAO_ROOT)/bin/cidlc -I$(CIAO_ROOT)/ciao -I($TAO_ROOT) -I$(TAO_ROOT)/tao -I$(TAO_ROOT)/orbsvcs/orbsvcs --
    inputext          = cidl
    source_outputext  = _svnt.cpp
    generic_outputext = E.idl
  }

}
