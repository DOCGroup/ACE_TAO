project : qt-min {
  Define_Custom(MOC) {
    automatic        = 0
    command          = perl
    commandflags     = $(ACE_ROOT)/bin/pch_cmd.pl -t -o -c $(QTDIR)/bin/moc
    output_option    = -o
    pch_option       = -p
    inputext         = .h
    pre_extension    = _moc
    source_outputext = .cpp
  }
}
