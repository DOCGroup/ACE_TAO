## Makefile.catior.am

if BUILD_CORBA_MESSAGING
if BUILD_EXCEPTIONS
if BUILD_RT_CORBA

bin_PROGRAMS = catior

catior_CPPFLAGS = \
  -I$(ACE_ROOT) \
  -I$(ACE_BUILDDIR) \
  -I$(TAO_ROOT) \
  -I$(TAO_BUILDDIR) \
  -I$(TAO_ROOT)/orbsvcs \
  -I$(TAO_BUILDDIR)/orbsvcs

