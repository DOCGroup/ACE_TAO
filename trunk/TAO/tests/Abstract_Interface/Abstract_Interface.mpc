// -*- MPC -*-
// $Id$

project(*client) : taoclient, valuetype {
  idlflags += -Sc

  IDL_Files {
    test.idl
  }

  Source_Files {
    client.cpp
    testC.cpp
  }

  Header_Files {
    testC.h
  }
}

project(*server) : taoserver, valuetype {
  after += *client
  idlflags += -Sc

  IDL_Files {
    test.idl
  }

  Source_Files {
    server.cpp
    test_i.cpp
    testC.cpp
    testS.cpp
  }

  Header_Files {
    test_i.h
    testC.h
    testS.h
  }
}
