// -*- MPC -*-
// $Id$

project(*Nested_Upcall_Idl): taoidldefaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*Nested_Upcall_Server): taoexe, portableserver {
  after += *Nested_Upcall_Idl
  Source_Files {
    server_i.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}

project(*Nested_Upcall_Client): taoexe, portableserver {
  after += *Nested_Upcall_Idl
  after += *Nested_Upcall_Server
  Source_Files {
    client_i.cpp
    client.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}
