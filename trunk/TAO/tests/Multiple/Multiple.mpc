// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  idlflags += -Gd
  IDL_Files {
    Multiple.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl

  Source_Files {
    Multiple_Impl.cpp
    Collocation_Tester.cpp
    server.cpp
  }
  Source_Files {
     MultipleC.cpp
     MultipleS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoserver {
  exename = client
  after += *idl

  Source_Files {
    Multiple_Impl.cpp
    Collocation_Tester.cpp
    client.cpp
  }
  Source_Files {
     MultipleC.cpp
     MultipleS.cpp
  }
  IDL_Files {
  }
}
