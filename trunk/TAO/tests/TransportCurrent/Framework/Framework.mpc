// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Current_Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, tc, pi_server {
  after += *idl
  includes += .
  Source_Files {
    server.cpp
    Current_Test_Impl.cpp
    Server_Request_Interceptor.cpp
    Tester.cpp
    Server_Main.cpp
  }
  Source_Files {
    Current_TestC.cpp
    Current_TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, tc, pi, dynamicinterface {
  after += *idl
  after += *Server
  includes += .
  Source_Files {
    Client_Request_Interceptor.cpp
    client.cpp
    Tester.cpp
  }
  Source_Files {
    Current_TestC.cpp
  }
  IDL_Files {
  }
}
