// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1;
}

project(*Server): taoserver, anytypecode, avoids_minimum_corba, bidir_giop {
  after += *idl
  Source_Files {
    test_i.cpp
    server.cpp
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoserver, anytypecode, avoids_minimum_corba, bidir_giop {
  exename = client
  after += *idl
  after += *Server
  Source_Files {
    test_i.cpp
    client.cpp
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}
