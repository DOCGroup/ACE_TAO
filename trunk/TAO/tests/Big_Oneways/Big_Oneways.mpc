project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    TestC.cpp
    TestS.cpp
    Coordinator.cpp
    Session_Control.cpp
    server.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoserver {
  exename = client
  after += *idl
  Source_Files {
    TestC.cpp
    TestS.cpp
    Peer.cpp
    Session.cpp
    Session_Task.cpp
    client.cpp
  }
  IDL_Files {
  }
}
