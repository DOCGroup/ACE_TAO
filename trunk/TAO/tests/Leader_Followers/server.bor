
#
# Makefile for building the Leader_Followers tests
# 

NAME = server
  
OBJFILES = \
        $(OBJDIR)\testC.obj \
	$(OBJDIR)\testS.obj \
	$(OBJDIR)\test_i.obj \
	$(OBJDIR)\server.obj
  
CFLAGS = $(ACE_CFLAGS) $(TAO_CFLAGS)

LIBFILES = $(ACE_LIB) $(TAO_LIB)

CPPDIR = .;..

IDLDIR = .
  
!include <$(ACE_ROOT)\include\makeinclude\build_example.bor>
  
# 
# IDL Build rules
# 
  
$(IDLDIR)\testC.cpp $(IDLDIR)\testS.cpp: $(IDLDIR)\test.idl
	$(TAO_IDL) $**

