// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    Coordinator.cpp
    Session_Control.cpp
    server.cpp
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoexe, portableserver {
  after += *idl
  after += *Server
  Source_Files {
    Peer.cpp
    Session.cpp
    Session_Task.cpp
    client.cpp
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}
