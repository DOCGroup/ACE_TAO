#
# Makefile for building the AMI simple_client
#

NAME = simple_client

TAO_IDL = $(CORE_BINDIR)\tao_idl -g $(CORE_BINDIR)\gperf.exe -Ge 1 -GC

OBJFILES = \
	$(OBJDIR)\ami_testS.obj \
	$(OBJDIR)\ami_testC.obj \
	$(OBJDIR)\simple_client.obj

CFLAGS = \
	$(ACE_CFLAGS) \
	$(TAO_CFLAGS) \
	$(TAO_PORTABLESERVER_CFLAGS)

LIBFILES = \
	$(ACE_LIB) \
	$(TAO_LIB) \
	$(TAO_PORTABLESERVER_LIB)

CPPDIR = .

IDLDIR = .

IDLFILES = \
        $(IDLDIR)\ami_test.idl

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

#
# IDL Build rules
#

$(IDLDIR)\ami_testS.cpp $(IDLDIR)\ami_testC.cpp: $(IDLDIR)\ami_test.idl
	$(TAO_IDL) $**
