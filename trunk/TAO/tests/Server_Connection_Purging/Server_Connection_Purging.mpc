// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only =1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    Test_i.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, anytypecode {
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}

project(*ShutCl): taoclient, anytypecode {
  after += *idl
  exename = shutdown_client
  Source_Files {
    client2.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}
