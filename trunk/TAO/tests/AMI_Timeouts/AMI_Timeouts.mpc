project(*Server): messaging, taoserver {
  idlflags -= -Sc
  idlflags += -GC
  requires += ami

  Source_Files {
    timeout_i.cpp
    timeout_client.cpp
    server.cpp
  }  
}
 
project(*Client): messaging, taoserver {
  exename   = client
  idlflags -= -Sc
  idlflags += -GC
  requires += ami

  Source_Files {
    timeout_i.cpp
    timeout_client.cpp
    client.cpp
  }  
}

