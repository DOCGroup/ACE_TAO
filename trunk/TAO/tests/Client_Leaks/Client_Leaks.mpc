// -*- MPC -*-
// $Id$

project(*Server): taoserver {
  Source_Files {
    Process_Factory.cpp
    Startup_Callback.cpp
    Server_Task.cpp
    server.cpp
  }
}

project(*Client): taoclient, anytypecode {
  after += *Server
  Source_Files {
    TestC.cpp
    Client_Task.cpp
    client.cpp
  }
}

project(*Child): taoserver {
  exename = child
  after += *Client
  Source_Files {
    Process.cpp
    child.cpp
  }
}

