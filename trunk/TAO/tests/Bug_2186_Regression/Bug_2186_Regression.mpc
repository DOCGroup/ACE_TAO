// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, strategies {
  after += *idl
  Source_Files {
    Hello.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoserver, strategies {
  after += *idl
  exename = client
  Source_Files {
    Hello.cpp
    client.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

