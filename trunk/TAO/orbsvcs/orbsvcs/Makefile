ifndef TAO_ROOT
  TAO_ROOT = $(ACE_ROOT)/TAO
endif # ! TAO_ROOT

MAKEFILE = Makefile
LIBNAME  = liborbsvcs
LIB      = $(LIBNAME).a
SHLIB    = $(LIBNAME).$(SOEXT)

# On non-Windows environment, we should at least define
# the export_include IDL flag.
TAO_IDLFLAGS = -Wb,export_macro=TAO_ORBSVCS_Export -Wb,export_include=orbsvcs_export.h

IDL_FILES = \
	CosNamingC \
	CosNamingS \
