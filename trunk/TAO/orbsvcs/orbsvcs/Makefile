	-I$(TOOLKIT) \
	$(CC_INCLUDES) \
	$(CC_FLAGS) \
	$(CC_DEFINES) \
	$(CC_EH) \
	$(CC_MT)

ifndef exceptions
CPPFLAGS += -DOS_NO_EXCEPTION_SPECIFIER
else
CPPFLAGS += -DOS_STL_ASSERT
endif #exceptions

endif #trader

