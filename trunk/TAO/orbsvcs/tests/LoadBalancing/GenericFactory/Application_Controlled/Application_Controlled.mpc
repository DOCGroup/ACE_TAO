project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*server): orbsvcsexe, portablegroup, loadbalancing, naming, svc_utils {
  after += *idl
  exename = server
  Source_Files {
	TestC.cpp
	TestS.cpp
	server.cpp
	LB_server.cpp
	Basic.cpp
  }
  IDL_Files {
  }
}
  
project(*client): orbsvcsexe, portablegroup, loadbalancing, naming, svc_utils {
  after += *idl
  exename = client
  Source_Files {
	TestC.cpp
	client.cpp
  }
  IDL_Files {
  }
}
