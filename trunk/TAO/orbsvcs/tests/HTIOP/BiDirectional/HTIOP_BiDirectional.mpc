project(*idl): taoidldefaults {
  IDL_Files {
    test.idl
  }

  custom_only = 1
}

project(*Server): taoexe, portableserver, bidir_giop, avoids_corba_e_micro {
  after += *idl
  exename = server
  Source_Files {
    testC.cpp
    testS.cpp
    test_i.cpp
    server.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoexe, portableserver, avoids_minimum_corba, bidir_giop, avoids_corba_e_micro {
  after += *idl
  exename = client
  Source_Files {
    testC.cpp
    testS.cpp
    test_i.cpp
    client.cpp
  }
  IDL_Files {
  }
}
