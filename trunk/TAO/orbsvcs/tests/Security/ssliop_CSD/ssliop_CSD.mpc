project(*idl): taoidldefaults {
  IDL_Files {
    Messenger.idl
  }
  custom_only = 1
}

project(*Server): orbsvcsexe, portableserver, security, ssl {
  Source_Files {
    Messenger_i.cpp
    MessengerServer.cpp
    MessengerC.cpp
    MessengerS.cpp
