project(*sequence server): taoexe, messaging, strategies {
  idlflags += -GC
  requires += ami

  Source_Files {
    Roundtrip.cpp
    Server_Task.cpp
    server.cpp
  }
}

project(*sequence client): taoexe, messaging, strategies {
  exename   = client
  idlflags += -GC
  requires += ami

  Source_Files {
    Roundtrip_Handler.cpp
    client.cpp
  }
}

