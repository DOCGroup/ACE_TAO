// -*- MPC -*-
// $Id$

project(*server): taoserver {
  idlflags += -Sa -St
  Source_Files {
    Factory.cpp
    Simple.cpp
    server.cpp
  }
}

project(*client): taoclient {
  Source_Files {
    TestC.cpp
    client.cpp
  }
}

project(*collocated): taoserver {
  idlflags += -Sa -St
  exename = collocated
  Source_Files {
    Simple.cpp
    collocated.cpp
  }
}

