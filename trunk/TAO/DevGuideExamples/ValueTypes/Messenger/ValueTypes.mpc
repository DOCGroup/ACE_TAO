// $Id$

project(*idl): taoidldefaults, valuetype {
  idlflags += -Wb,pch_include=_pch.h
  IDL_Files {
    Messenger.idl
  }
  custom_only = 1
}

project(*Server): taoserver, valuetype, avoids_minimum_corba {
  exename = MessengerServer
  after += *idl
  Source_Files {
    Messenger_i.cpp
    Message_i.cpp
    MessengerServer.cpp
  }
  Source_Files {
    MessengerC.cpp
    MessengerS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, valuetype, avoids_minimum_corba {
  exename = MessengerClient
  after += *idl
  Source_Files {
    Message_i.cpp
    MessengerClient.cpp
  }
  Source_Files {
    MessengerC.cpp
  }
  IDL_Files {
  }
}
