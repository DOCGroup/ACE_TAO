project(*Server): csd_threadpool, taoserver, pi {
  exename = server_main

  Source_Files {
    server_main.cpp
    Foo_i.cpp
    FooC.cpp
    FooS.cpp
    Callback_i.cpp
    CallbackC.cpp
    CallbackS.cpp
    ServerApp.cpp
    ClientTask.cpp
    FooServantList.cpp
    OrbTask.cpp
    OrbShutdownTask.cpp
  }
}

project(*Client): csd_threadpool, taoserver, pi {
  exename = client_main
  after += *Server

  Source_Files {
    client_main.cpp
    Callback_i.cpp
    CallbackC.cpp
    CallbackS.cpp
    FooC.cpp
    ClientTask.cpp
    ClientApp.cpp
  }
}

