// -*- MPC -*-
// $Id$

project(*IDL): taoidldefaults {
  IDL_Files {
    Grid.idl
  }
  custom_only = 1
}

project(*server): namingexe, iortable, utils {
  exename = server
  after += *IDL
  Source_Files {
    Grid_i.cpp
    server.cpp
    ../Simple_util.cpp
    GridS.cpp
    GridC.cpp
  }
  IDL_Files {
  }
}

project(*client): namingexe, iortable, utils {
  exename = client
  after += *IDL
  Source_Files {
    client.cpp
    ../Simple_util.cpp
    Grid_Client_i.cpp
    GridC.cpp
  }
  IDL_Files {
  }
}
