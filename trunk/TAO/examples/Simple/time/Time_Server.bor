!include <$(ACE_ROOT)\include\makeinclude\build_example.bor>

#
# IDL Build rules
#

$(IDLDIR)\TimeC.cpp $(IDLDIR)\TimeS.cpp: $(IDLDIR)\Time.idl
	$(TAO_IDL) $**

