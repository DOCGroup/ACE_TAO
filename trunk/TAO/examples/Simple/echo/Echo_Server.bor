#
# Makefile for building the Echo example
#

NAME = Echo_Server

OBJFILES = \
	$(OBJDIR)\EchoC.obj \
	$(OBJDIR)\EchoS.obj \
	$(OBJDIR)\Echo_i.obj \
	$(OBJDIR)\Simple_util.obj \
	$(OBJDIR)\server.obj

IDLFILES = \
        $(IDLDIR)\Echo.idl

CPPDIR = .;..

IDLDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_example.bor>

#
# IDL Build rules
#

$(IDLDIR)\EchoC.cpp $(IDLDIR)\EchoS.cpp: $(IDLDIR)\Echo.idl
	$(TAO_IDL) $**

