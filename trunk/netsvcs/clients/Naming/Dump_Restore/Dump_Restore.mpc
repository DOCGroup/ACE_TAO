project(Netsvcs_Dump_Restore_Lib): acelib {
  sharedname    = Dump_Restore
  dynamicflags += ACE_DUMP_RESTORE_BUILD_DLL
  Source_Files {
    Dump_Restore.cpp
  }
}

project(Netsvcs_Dump_Restore) : aceexe {
  exename = main
  libs    += Dump_Restore
  after   += Netsvcs_Dump_Restore_Lib
  Source_Files {
    main.cpp
  }
}
