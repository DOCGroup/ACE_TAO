// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*NsMain): namingexe, naming_serv {
  exename = nsmain
  after += *idl
  Source_Files {
    Hello.cpp
    NamingTask.cpp
    nsmain.cpp
    NsShutdown.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
}

project(*Client): namingexe {
  exename = client
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
}

project(*Server): namingexe, portableserver {
  exename = server
  after += *idl
  Source_Files {
    server.cpp
    Hello.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
}

