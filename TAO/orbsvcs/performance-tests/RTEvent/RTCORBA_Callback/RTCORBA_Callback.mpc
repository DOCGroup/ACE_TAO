// $Id$

project(*IDL): orbsvcsexe, rtcorbaevent, strategies, avoids_minimum_corba {
  IDL_Files {
    Test.idl
  }

  custom_only = 1
}

project(*server): orbsvcsexe, rtcorbaevent, strategies, avoids_minimum_corba {
  after    += RTEC_Perf
  libs     += TAO_RTEC_Perf

  specific (automake) {
    includes += $(srcdir)/../lib
  } else {
    includes += ../lib
  }

  Source_Files {
    server.cpp
    Session.cpp
    Session_Factory.cpp
    TestS.cpp
    TestC.cpp
  }

  after    += *IDL
  IDL_Files {
  }
}

project(*client): orbsvcsexe, rtcorbaevent, strategies, avoids_minimum_corba {
  after    += RTEC_Perf
  libs     += TAO_RTEC_Perf

  specific (automake) {
    includes += $(srcdir)/../lib
  } else {
    includes += ../lib
  }

  Source_Files {
    client.cpp
    Callback.cpp
    TestS.cpp
    TestC.cpp
  }

  after    += *IDL
  IDL_Files {
  }
}
