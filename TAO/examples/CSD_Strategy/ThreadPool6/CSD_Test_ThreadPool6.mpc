// -*- MPC -*-
// $Id$

project(*Server): csd_threadpool, taoserver, avoids_minimum_corba, pi {
  exename = server_main
  idlflags += -GT

  Source_Files {
    Foo_i.cpp
    ServerApp.cpp
    server_main.cpp
    OrbShutdownTask.cpp
  }
}

project(*Client): taoexe, anytypecode {
  exename = client_main
  after += *Server

  Source_Files {
    FooC.cpp
    ClientApp.cpp
    client_main.cpp
  }
}

