  }
}

project(AMI_InterOutArgsT_stub) : ccm_stub, ami, ami4ccm_stub {
  after += AMI_InterOutArgsT_lem_gen
  libs  +=
  libout = ../lib
  libpaths += ../lib
  includes +=   ..

  sharedname = InterOutArgsT_stub
  dynamicflags = INTEROUTARGS_T_STUB_BUILD_DLL

  IDL_Files {
  }

  Source_Files {
    InterOutArgsTC.cpp
    InterOutArgsTAC.cpp
  }

  Header_Files {
    InterOutArgsTC.h
    InterOutArgsT_stub_export.h
}

  Inline_Files {
    InterOutArgsTC.inl
  }
}

