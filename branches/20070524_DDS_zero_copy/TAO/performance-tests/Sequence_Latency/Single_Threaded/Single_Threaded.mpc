project(*sequence_idl): taoidldefaults, strategies {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*sequence server) : taoserver, strategies {
  after += *sequence_idl
  Source_Files {
    Roundtrip.cpp
    TestS.cpp
    TestC.cpp
    server.cpp
  }
  IDL_Files {
  }
}

project(*sequence client): taoclient, strategies {
  after += *sequence_idl
  avoids += ace_for_tao
  Source_Files {
    TestC.cpp
    client.cpp
  }
  IDL_Files {
  }
}
