project(*idl): taoidldefaults, ami {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, ami, iortable {
  after += *idl
  Source_Files {
    Hello.cpp
    Server_Task.cpp
    server.cpp
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoserver, ami {
  exename = client
  after += *idl
  Source_Files {
    Client_Task.cpp
    Reply_Handler.cpp
    client.cpp
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}
