project(*idl): taoidldefaults, smart_proxies {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, smart_proxies {
  after += *idl
  Source_Files {
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  IDL_Files {
