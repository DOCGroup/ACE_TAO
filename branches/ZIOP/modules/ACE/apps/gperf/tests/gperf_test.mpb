// -*- MPC -*-
// $Id$

project: aceexe, crosscompile {
  after += gperf
  avoids += uses_wchar
  Define_Custom(GPERF) {
    command          = $(ACE_ROOT)/bin/gperf
    libpath         += $(ACE_ROOT)/lib
    output_option    = >
    inputext         = .gperf
    source_outputext = .cpp
  }

  Source_Files {
    test.cpp
  }
}
