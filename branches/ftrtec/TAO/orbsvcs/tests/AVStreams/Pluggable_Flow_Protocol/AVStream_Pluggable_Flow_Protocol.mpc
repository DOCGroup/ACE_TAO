project(*ts) : orbsvcslib, portableserver, avstreams, naming {
  sharedname   = TAO_TS
  dynamicflags = TAO_AV_BUILD_DLL
  comps       += AV

  Source_Files {
    TimeStamp.cpp
  }
}

project(*sender): avstreamsexe {
  exename  = sender

  Source_Files {
    sender.cpp
  }
}

project(*receiver): avstreamsexe {
  exename  = receiver

  Source_Files {
    receiver.cpp
  }
}

