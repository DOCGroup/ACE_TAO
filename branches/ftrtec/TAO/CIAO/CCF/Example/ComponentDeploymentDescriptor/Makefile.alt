root = ../..

include $(root)/Bootstrap.rules

$(call include, $(root)/Config.rules)

$(call include, $(UTILITY_BUILD_RULES)/Executable.pre.rules)

cxx_translation_units := SyntaxTree.cpp Traversal.cpp cdd.cpp

