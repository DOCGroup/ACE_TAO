// -*- MPC -*-
// $Id$

project(*Server): taoserver {
  avoids += ace_for_tao
  Source_Files {
    test_i.cpp
    server.cpp
  }
}

project(*Client): taoclient, anytypecode {
  avoids += ace_for_tao
  after += *Server
  Source_Files {
    testC.cpp
    client.cpp
  }
}

project(*OctetSeq): taoclient, anytypecode {
  exename = OctetSeq
  after += *Client
  Source_Files {
    testC.cpp
    OctetSeq.cpp
  }
}

