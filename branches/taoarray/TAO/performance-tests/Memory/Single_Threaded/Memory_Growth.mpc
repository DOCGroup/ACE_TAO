// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  idlflags += -Sa -St

  IDL_Files {
    Test.idl
  }

  custom_only = 1
}

project(*server): taoserver {
  after += *idl
  Source_Files {
    Memory_Growth.cpp
    server.cpp
    TestS.cpp
    TestC.cpp
  }
  IDL_Files {
  }
}

project(*client): taoclient {
  after += *idl
  Source_Files {
    TestC.cpp
    client.cpp
  }
  IDL_Files {
  }
}

