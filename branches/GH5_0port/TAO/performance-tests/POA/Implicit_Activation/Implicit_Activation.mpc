// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  idlflags += -Sa -St

  IDL_Files {
    Test.idl
  }

  custom_only = 1
}

project(*server): taoserver {
  after += *idl

  Source_Files {
    Factory.cpp
    Simple.cpp
    server.cpp
    TestS.cpp
    TestC.cpp
  }

  IDL_Files {
  }
}

project(*client): taoclient {
  avoids += ace_for_tao
  after  += *idl

  Source_Files {
    TestC.cpp
    client.cpp
  }

  IDL_Files {
  }
}

project(*collocated): taoserver {
  avoids += ace_for_tao
  after  += *idl
  exename = collocated
  Source_Files {
    Simple.cpp
    collocated.cpp
    TestS.cpp
    TestC.cpp
  }

  IDL_Files {
  }
}

