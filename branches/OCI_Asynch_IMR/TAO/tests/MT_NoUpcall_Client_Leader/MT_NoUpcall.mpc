// -*- MPC -*-
// $Id$


project(*idl): taoidldefaults {
  idlflags += -Sp
  IDL_Files {
    SharedIntf.idl
  }
  custom_only = 1
}

project(*Server): messaging, taoserver, threads {
  exename = server
  after += *idl
  Source_Files {
    SharedIntf_i.cpp
    server.cpp
    worker.cpp
    chatter.cpp
    police.cpp
  }
  Source_Files {
    SharedIntfC.cpp
    SharedIntfS.cpp
  }
  IDL_Files {
  }
}

project(*Client): messaging, taoserver, threads {
  exename = client
  after += *idl
  Source_Files {
    SharedIntf_i.cpp
    client.cpp
    worker.cpp
    chatter.cpp
    police.cpp
  }
  Source_Files {
    SharedIntfC.cpp
    SharedIntfS.cpp
  }
  IDL_Files {
  }
}

