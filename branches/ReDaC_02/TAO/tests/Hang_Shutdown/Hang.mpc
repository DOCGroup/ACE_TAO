// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    server.cpp
    TestS.cpp
    TestC.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, anytypecode {
  avoids += ace_for_tao
  after  += *Server
  Source_Files {
    TestC.cpp
    client.cpp
  }
  IDL_Files {
  }
}
