project(Rand_Work_Node1): orbsvcsexe, rtevent, rtsched, rtschedevent, rtrgkokyuevent, rgkokyu {
  exename = Node1_EC
  idlflags += -I$(TAO_ROOT) -I$(TAO_ROOT)/orbsvcs -Ge 1 -Wb,pre_include=ace/pre.h -Wb,post_include=ace/post.h
  libs += wrappers
  includes += $(TAO_ROOT)/orbsvcs
  macros += KOKYU_HAS_RELEASE_GUARD

  Source_Files {
    Kokyu_EC.cpp
    Node1_EC.cpp
    Consumer.cpp
    Supplier.cpp
    federated_dsui_vars.cpp
  }
}

