project(Consumer_Fan): orbsvcsexe, rtevent, rtsched, rtschedevent, rtrgkokyuevent, rgkokyu {
  exename = Consumer_EC
  idlflags += -I$(TAO_ROOT) -I$(TAO_ROOT)/orbsvcs -Ge 1 -Wb,pre_include=ace/pre.h -Wb,post_include=ace/post.h
  libs += wrappers
  includes += $(TAO_ROOT)/orbsvcs
  macros += KOKYU_HAS_RELEASE_GUARD

  Source_Files {
    Kokyu_EC.cpp
    Consumer_EC.cpp
    Consumer.cpp
    Supplier.cpp
    federated_dsui_vars.cpp
  }
}

project(Supplier_Fan): orbsvcsexe, rtevent, rtsched, rtschedevent, rtrgkokyuevent, rgkokyu {
