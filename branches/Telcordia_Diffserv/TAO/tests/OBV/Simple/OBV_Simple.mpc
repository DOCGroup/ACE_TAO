// -*- MPC -*-
// $Id$

project(*IDL): taoidldefaults {
  IDL_Files {
    OBV.idl
  }
  custom_only = 1
}

project(*server): taoserver, utils, valuetype {
  exename = server
  after += *IDL
  Source_Files {
    Server_i.cpp
    OBV_impl.cpp
    server.cpp
    OBVS.cpp
    OBVC.cpp
  }
  IDL_Files {
  }
}

project(*client): taoserver, utils, valuetype {
  exename = client
  after += *IDL
  Source_Files {
    Client_i.cpp
    OBV_impl.cpp
    client.cpp
    OBVS.cpp
    OBVC.cpp
  }
  IDL_Files {
  }
}
