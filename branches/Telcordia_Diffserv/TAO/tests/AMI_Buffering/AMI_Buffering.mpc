// -*- MPC -*-
// $Id$

project(*Server): taoserver, ami {
  Source_Files {
    AMI_Buffering.cpp
    server.cpp
  }
}

project(*Client): taoserver, ami {
  after += *Server
  exename = client
  Source_Files {
    Client_Task.cpp
    Reply_Handler.cpp
    client.cpp
  }
}

project(*Admin): taoserver, ami {
  after += *Client
  exename = admin
  Source_Files {
    AMI_Buffering_Admin.cpp
    admin.cpp
  }
}
