  <arms:switchList>

    <!-- ==================================================
	 A network configuration has one or more switches/routers
	 ================================================== -->

    <!-- A switch can be a Layer2 Switch or Layer 3 Router -->
    <arms:switch>
      <!-- If L3 capable, it is a router or it is a Layer 2 switch -->
      <arms:isL3Capable>true</arms:isL3Capable>  
      
      <!-- is it an edge router/switch? -->
      <arms:isItEdge>true</arms:isItEdge>     

      <!-- internalID of the switch/router -->
      <arms:internalID>Switch1</arms:internalID>    
      <!--managedElement is a fully qualified DNS name for the
           router/switch so as to to able to talk to the switch/router
           by the flowProvisioner; -->
      <arms:managedElementID>192.168.81.1</arms:managedElementID>         
      
      <!-- user ID, password, enble pasword are for logging into the
           router/swtich for configuring and enabling new switch
