// $Id$

project : mfc, regsvr32 {
  pch_header = StdAfx.h
  pch_source = StdAfx.cpp

  includes += . $(GME_ROOT)/SDK/BON $(GME_ROOT)

  requires += gme

  specific (vc6, vc7, vc71, vc8, nmake) {
    dllout = $(COSMIC_ROOT)/bin
    libout = $(COSMIC_ROOT)/lib

    midl_includes += $(GME_ROOT)Interfaces

    Debug::runtime_library = 3
    Release::runtime_library = 2
  }
}
