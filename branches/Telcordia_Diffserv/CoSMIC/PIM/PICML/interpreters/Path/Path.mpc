project(Path): picmllib {
   after += PICML
   sharedname = Path
   macros += PATH_BUILD_DLL PICML_HAS_DLL
   includes += $(COSMIC_ROOT)/PIM/PICML/interpreters/PICML $(BOOST_ROOT)/include/$(BOOST_VERSION)
   libs += PICML

   Source_Files {
     PathVisitor.cpp
   }

   Header_Files {
     PathVisitor.h
     Path_Export.h
   }
}
