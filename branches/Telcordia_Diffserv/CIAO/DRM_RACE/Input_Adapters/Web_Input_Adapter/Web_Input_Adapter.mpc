          Plan_Analyzer_Interface_stub \
          web_common_stub \
          Web_Input_Adapter_interface

  IDL_Files {
    Web_Input_Adapter.idl
  }

  Source_Files {
    Web_Input_AdapterC.cpp
  }

  Header_Files {
    Web_Input_AdapterC.h
  }

  Inline_Files {
    Web_Input_AdapterC.inl
  }
}

