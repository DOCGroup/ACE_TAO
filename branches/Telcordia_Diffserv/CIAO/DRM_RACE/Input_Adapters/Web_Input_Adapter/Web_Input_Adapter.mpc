              -Wb,stub_export_macro=WEB_INPUT_ADAPTER_INTERFACE_Export \
              -Wb,stub_export_include=Web_Input_Adapter_interface_export.h \
              -Wb,skel_export_macro=WEB_INPUT_ADAPTER_SVNT_Export \
              -Wb,skel_export_include=Web_Input_Adapter_svnt_export.h
  dynamicflags  = WEB_INPUT_ADAPTER_INTERFACE_BUILD_DLL

  IDL_Files {
    Web_Input_Adapter_Interface.idl
  }

  Source_Files {
    Web_Input_Adapter_InterfaceC.cpp
  }

  Header_Files {
    Web_Input_Adapter_InterfaceC.h
  }

  Inline_Files {
    Web_Input_Adapter_InterfaceC.inl
  }
}

