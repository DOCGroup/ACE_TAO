// -*- MPC -*-
// $Id$

project(*Server): taoserver, messaging, ami {
  exename = server
  Source_Files {
    ami_test_i.cpp
    server.cpp
  }
}

project(*Client): messaging, taoexe, portableserver, ami {
  exename = client
  after += *Server
  Source_Files {
    client.cpp
  }
}

project(*Simple Client): messaging, taoexe, portableserver, ami {
  exename = simple_client
  after += *Server
  Source_Files {
    simple_client.cpp
  }
}
