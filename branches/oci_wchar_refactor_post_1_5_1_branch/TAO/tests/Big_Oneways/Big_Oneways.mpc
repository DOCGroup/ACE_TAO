// -*- MPC -*-
// $Id$

project(*Server): taoserver {
  exename = server
  Source_Files {
    Coordinator.cpp
    Session_Control.cpp
    server.cpp
  }
}

project(*Client): taoexe, portableserver {
  exename = client
  after += *Server
  Source_Files {
    Peer.cpp
    Session.cpp
    Session_Task.cpp
    client.cpp
  }
}

