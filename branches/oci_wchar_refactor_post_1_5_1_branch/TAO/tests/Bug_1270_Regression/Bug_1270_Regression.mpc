// -*- MPC -*-
// $Id$

project(*Server): taoserver, messaging {
  exename = server
  Source_Files {
    Echo_Caller.cpp
    Server_Timer.cpp
    server.cpp
  }
}

project(*Client): taoclient, messaging {
  exename = client
  after += *Server
  Source_Files {
    Echo.cpp
    client.cpp
  }
}

project(*Shutdown): taoexe, messaging {
  exename = shutdown
  after += *Client
  Source_Files {
    shutdown.cpp
  }
}
