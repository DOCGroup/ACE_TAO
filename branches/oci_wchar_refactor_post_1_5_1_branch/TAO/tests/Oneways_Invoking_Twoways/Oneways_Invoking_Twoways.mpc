// -*- MPC -*-
// $Id$

project(*Server): taoserver {
  exename = server
  Source_Files {
    Server_Task.cpp
    Sender_i.cpp
    server.cpp
  }
}

project(*Client): taoexe, portableserver {
  exename = client
  after += *Server
  Source_Files {
    Client_Task.cpp
    Server_Task.cpp
    Receiver_i.cpp
    client.cpp
  }
}

