// -*- MPC -*-
// $Id$

project(*Server): taoserver {
  exename = server
  idlflags += -Gd

  Source_Files {
    Multiple_Impl.cpp
    Collocation_Tester.cpp
    server.cpp
  }
}

project(*Client): taoexe, portableserver {
  exename = client
  idlflags += -Gd
  after += *Server

  Source_Files {
    Multiple_Impl.cpp
    Collocation_Tester.cpp
    client.cpp
  }
}

