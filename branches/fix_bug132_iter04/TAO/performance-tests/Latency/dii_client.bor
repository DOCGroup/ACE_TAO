#
# Makefile for building the deferred_synch_client latency performance-tests
#

NAME = deferred_synch_client

TAO_IDL = $(CORE_BINDIR)\tao_idl -g $(CORE_BINDIR)\gperf.exe -Ge 1 -GC

OBJFILES = \
	$(OBJDIR)\testC.obj \
	$(OBJDIR)\dii_client.obj

CFLAGS = \
	$(ACE_CFLAGS) \
	$(TAO_CFLAGS) \
	$(TAO_STRATEGIES_CFLAGS) \
	$(TAO_DYNAMICINTERFACE_CFLAGS)

LIBFILES = \
	$(ACE_LIB) \
	$(TAO_LIB) \
	$(TAO_STRATEGIES_LIB) \
	$(TAO_DYNAMICINTERFACE_LIB)

IDLFILES = \
        $(IDLDIR)\test.idl

CPPDIR = .

IDLDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

#
# IDL Build rules
#

$(IDLDIR)\testS.cpp $(IDLDIR)\testC.cpp: $(IDLDIR)\Test.idl
	$(TAO_IDL) $**
