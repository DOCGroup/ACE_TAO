  Source_Files {
    Foo_BC.cpp
    Foo_BS.cpp
    Foo_B_ClientEngine.cpp
    Foo_B_SimpleClientEngine.cpp
    Foo_B_i.cpp
    Foo_B_Statistics.cpp
    Callback_i.cpp
    CallbackC.cpp
    CallbackS.cpp
  }

}
