// -*- MPC -*-
// $Id$

project {
  Define_Custom(SWIG) {
    automatic         = 1
    command           = swig
    commandflags      = -tcl -c++
    inputext          = .swg, .swig, .i
    source_outputext  = _wrap.cxx
  }
  requires += swig_tcl
  includes += $(TCL_INCLUDE)/.
  libpaths += $(TCL_LIBPATH)/.
}
