// $Id$

project(Base_obo_stub) : taoidldefaults, ndds_ts_defaults, typecode, nddslib {
  sharedname = Base_obo_stub
  dynamicflags += BASE_OBO_STUB_BUILD_DLL NDDS_USER_DLL_EXPORT
  libout = ../descriptors

  idlflags += -SS -Sci \
              -Wb,stub_export_macro=BASE_OBO_STUB_Export \
              -Wb,stub_export_include=Base_obo_stub_export.h

  IDL_Files {
    idlflags += -Gxhst
    Base_ObO.idl
  }

  NDDSTypeSupport_Files {
    Base_ObO.idl
  }

  Source_Files {
  }
}

