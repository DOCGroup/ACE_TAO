project(Test DLL): taolib_with_idl {
  sharedname = Test_DLL
  depends   += PortableServer
  libs       = TAO_PortableServer TAO
  idlflags  += -Wb,pre_include=ace/pre.h -Wb,post_include=ace/post.h
  dllflags   = OLT_BUILD_DLL

  Source_Files {
    Test_i.cpp
    Loader.cpp
  }
}

