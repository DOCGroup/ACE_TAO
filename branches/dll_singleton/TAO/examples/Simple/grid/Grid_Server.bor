#
# Makefile for building the Grid example
#

NAME = server

OBJFILES = \
	$(OBJDIR)\GridC.obj \
	$(OBJDIR)\GridS.obj \
	$(OBJDIR)\Grid_i.obj \
	$(OBJDIR)\Simple_util.obj \
	$(OBJDIR)\server.obj

IDLFILES = \
        $(IDLDIR)\Grid.idl

CPPDIR = .;..

IDLDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_example.bor>

#
# IDL Build rules
#

$(IDLDIR)\GridC.cpp $(IDLDIR)\GridS.cpp: $(IDLDIR)\Grid.idl
	$(TAO_IDL) $**

