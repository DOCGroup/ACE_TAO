// -*- MPC -*-
// $Id$

project(*server): taoserver, strategies {
  idlflags += -Sp -Sd

  Source_Files {
    UDP_Client_i.cpp
    server.cpp
    UDP_i.cpp
  }
}

project(*client): taoserver, strategies {
  exename = client
  after += *server
  idlflags += -Sp -Sd

  Source_Files {
    client.cpp
    UDP_Client_i.cpp
    UDP_i.cpp
  }
}

