module CCM_AMI
{
  struct InternalException
    {
      long id;
      string error_string;
    };

  exception InternalError
    {
      InternalException ex;
    };

  // Sender/Receiver interface
  interface MyFoo
    {
      long foo (in string in_str, out string answer)
        raises (InternalError);
    };
};
