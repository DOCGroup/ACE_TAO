// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Foo.idl
  }
  custom_only = 1
}

project(*Server): csd_threadpool, taoserver, pi, avoids_corba_e_micro {
  exename = server_main
  after += *idl

  Source_Files {
    FooS.cpp
    FooC.cpp
    Foo_i.cpp
    FooServantList.cpp
    ServerApp.cpp
    server_main.cpp
    OrbShutdownTask.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoexe, anytypecode {
  exename = client_main
  after += *idl

  Source_Files {
    FooC.cpp
    ClientApp.cpp
    client_main.cpp
  }
  IDL_Files {
  }
}
