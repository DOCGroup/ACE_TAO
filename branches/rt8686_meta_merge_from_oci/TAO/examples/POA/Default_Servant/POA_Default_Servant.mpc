// -*- MPC -*-
// $Id$

project(*IDL): taoidldefaults, anytypecode {
  IDL_Files {
    File.idl
  }
  custom_only = 1
}

project(*Client) : taoclient, anytypecode, minimum_corba {
  after += *IDL
  source_files {
    FileC.cpp
    client.cpp
  }
  IDL_Files {
  }
}

project(*Server) : taoserver, minimum_corba {
  after += *IDL
  source_files {
    File_i.cpp
    server.cpp
    FileS.cpp
    FileC.cpp
  }
  IDL_Files {
  }
}
