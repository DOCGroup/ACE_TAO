// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): messaging, taoserver {
  after += *idl
  Source_Files {
    Sleep_Service.cpp
    Server_Task.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): messaging, taoexe, portableserver {
  after += *idl
  after += *Server
  Source_Files {
    Client_Task.cpp
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}
