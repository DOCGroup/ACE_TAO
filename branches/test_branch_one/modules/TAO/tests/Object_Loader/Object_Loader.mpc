// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Test): taolib_with_idl, portableserver {
  after += *idl
  sharedname   = Test_DLL
  dynamicflags = OLT_BUILD_DLL

  IDL_Files {
  }

  Source_Files {
    TestC.cpp
    TestS.cpp
    Test_i.cpp
    Loader.cpp
  }
}

project(*Driver): taoserver {
  exename = driver
  after += *Test *idl

  IDL_Files {
  }

  Source_Files {
    TestC.cpp
    driver.cpp
  }
}
