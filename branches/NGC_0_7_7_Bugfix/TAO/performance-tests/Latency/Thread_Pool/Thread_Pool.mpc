project(*latency_idl): taoidldefaults, strategies {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*latency server): taoserver, strategies {
  after += *latency_idl
  Source_Files {
    Roundtrip.cpp
    TestS.cpp
    TestC.cpp
    Worker_Thread.cpp
    server.cpp
  }
