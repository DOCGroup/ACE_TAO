project : taoidldefaults {
  idlflags += -I$(CIAO_ROOT) -I$(CIAO_ROOT)/ccm

  Modify_Custom (IDL) {
    optional(source_pre_extension) {
      commandflags(-Gsv) += _svnt
    }

    optional(header_pre_extension) {
      commandflags(-Gsv) += _svnt
    }

    optional(generic_outputext) {
      commandflags(-Glem) += E.idl
    }
  }
}
