#
# Makefile for building the anyop performance-tests executable
#

NAME = anyop

TAO_IDL = $(CORE_BINDIR)\tao_idl -g $(CORE_BINDIR)\gperf.exe -Ge 1

OBJFILES = \
        $(OBJDIR)\testC.obj \
	$(OBJDIR)\anyop.obj

CFLAGS = \
	$(ACE_CFLAGS) \
	$(TAO_CFLAGS)

LIBFILES = \
	$(ACE_LIB) \
	$(TAO_LIB)

IDLFILES = \
        $(IDLDIR)\test.idl

CPPDIR = .

IDLDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

#
# IDL Build rules
#

$(IDLDIR)\testC.cpp: $(IDLDIR)\test.idl
	$(TAO_IDL) $**
