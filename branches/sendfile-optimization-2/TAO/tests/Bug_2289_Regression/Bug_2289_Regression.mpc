// -*- MPC -*-
// $Id$

project(*Server): taoserver, iortable {
  exename = server
  idlflags += -Gp -Gd -Sa -St
  Source_Files {
    server.cpp
    MyInterfaceImpl.cpp
  }
}

project(*Client): taoserver, iortable {
  exename = client
  idlflags += -Gp -Gd -Sa -St
  after += *Server
  Source_Files {
    client.cpp
    MyInterfaceImpl.cpp
  }
}
