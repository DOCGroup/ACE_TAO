// -*- MPC -*-
// $Id$

project(*idl) : taoclient, exceptions {
  IDL_Files {
    time.idl
  }
  custom_only = 1
}

project(*client) : taoclient, anytypecode, exceptions {
  after += *idl
  requires += corba_messaging
  source_files {
    client.cpp
    timeC.cpp
  }
  IDL_Files {
  }
}

project(*server) : taoserver, exceptions {
  after += *idl
  source_files {
    server.cpp
    timeS.cpp
    timeC.cpp
  }
  IDL_Files {
  }
}
