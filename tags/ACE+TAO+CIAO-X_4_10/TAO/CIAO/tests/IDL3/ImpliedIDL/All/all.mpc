project(ConvertIDL3) : taoidl3toidl2defaults, ciao_component_dnc {
  idlflags += -Sc -Sm
  
  IDL3TOIDL2_Files {
    convert.idl
    include.idl
    pass_through.idl
  }
  
  IDL_Files {
    convert_IDL2.idl
    include_IDL2.idl
    pass_through_IDL2.idl << include_IDL2.idl
  }
  
  Source_Files {
    convert_IDL2C.cpp
    convert_IDL2S.cpp
    include_IDL2C.cpp
    include_IDL2S.cpp
    pass_through_IDL2C.cpp
    pass_through_IDL2S.cpp
  }
}

