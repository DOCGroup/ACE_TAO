project(*poa idl): taoidldefaults {
  IDL_Files {
    demux_test.idl
  }
  custom_only = 1
}

project(*poa server): namingexe, taoserver, avoids_corba_e_micro {
  after += *poa_idl
  Source_Files {
    demux_test_server.cpp
    demux_test_i.cpp
    server.cpp
    demux_testS.cpp
    demux_testC.cpp
  }
  IDL_Files {
  }
}

project(*poa client): namingexe, taoserver {
  exename = client
  after += *poa_idl
  Source_Files {
    demux_test_client.cpp
    client.cpp
    demux_testS.cpp
    demux_testC.cpp
  }
  IDL_Files {
  }
}
