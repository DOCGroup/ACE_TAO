project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, messaging {
  after += *idl
  Source_Files {
    Oneway_Receiver.cpp
    Shutdown_Helper.cpp
    server.cpp
  }
  Header_Files {
    Oneway_Receiver.h
    Shutdown_Helper.h
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): messaging, taoserver {
  exename = client
  after += *idl
  Source_Files {
    client.cpp
  }
  Header_Files {
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}
