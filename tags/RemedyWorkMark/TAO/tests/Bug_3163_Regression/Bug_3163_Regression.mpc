// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  idlflags += -Sp
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient {
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
  IDL_Files {
  }
}

