<!-- $Id$ -->
<Deployment:DeploymentPlan
                          xmlns:Deployment="http://www.omg.org/Deployment"
                          xmlns:xmi="http://www.omg.org/XMI"
                          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                          xsi:schemaLocation="http://www.omg.org/Deployment Deployment.xsd">
  <label>Test_Depl</label>
  <UUID>Test_Depl</UUID>

  <implementation xmi:id="ComponentImplementation">
    <name>ComponentImplementation</name>
    <source/>
    <artifact xmi:idref="Component_ExecArtifact" />
    <artifact xmi:idref="Component_SvntArtifact" />
    <execParameter>
      <name>component factory</name>
      <value>
        <type>
          <kind>tk_string</kind>
        </type>
        <value>
          <string>create_Registration_Server_Registration_Impl</string>
        </value>
      </value>
    </execParameter>
    <execParameter>
      <name>edu.vanderbilt.dre.CIAO.ServerEntrypoint</name>
      <value>
        <type>
          <kind>tk_string</kind>
        </type>
        <value>
          <string>create_Registration_Server_Registration_Server</string>
        </value>
      </value>
    </execParameter>
    <execParameter>
      <name>edu.vanderbilt.dre.CIAO.ServerArtifact</name>
      <value>
        <type>
          <kind>tk_string</kind>
        </type>
        <value>
          <string>Server_Component_svnt</string>
        </value>
      </value>
    </execParameter>
    <execParameter>
      <name>edu.vanderbilt.dre.CIAO.ExecutorArtifact</name>
      <value>
        <type>
          <kind>tk_string</kind>
        </type>
        <value>
          <string>Server_Component_exec</string>
        </value>
      </value>
    </execParameter>
  </implementation>

  <instance xmi:id="Component">
    <name>Server_Component</name>
    <node>ComponentNode</node>
    <!-- hostname -->
    <source/>
    <implementation xmi:idref="ComponentImplementation" />
    <configProperty>
      <name>edu.vanderbilt.dre.DAnCE.RegisterNaming</name>
      <value>
        <type>
          <kind>tk_string</kind>
        </type>
        <value>
          <string>ComponentInstance</string>
        </value>
      </value>
    </configProperty>
  </instance>

  <artifact xmi:id="Component_ExecArtifact">
    <name>Server_Component_exec</name>
    <source/>
    <node/>
    <location>Server_Component_exec</location>
  </artifact>
  <artifact xmi:id="Component_SvntArtifact">
    <name>Server_Component_svnt</name>
    <source/>
    <node/>
    <location>Server_Component_svnt</location>
  </artifact>
</Deployment:DeploymentPlan>
