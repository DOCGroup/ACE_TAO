      <instance xmi:idref="ComponentInstance" />
    </internalEndpoint>
    <internalEndpoint>
      <portName>pull_observer_filter_config</portName>
      <provider>true</provider>
      <kind>Facet</kind>
      <instance xmi:idref="ThreeDim_CFT_Test_ConnectorInstance" />
    </internalEndpoint>
  </connection>

