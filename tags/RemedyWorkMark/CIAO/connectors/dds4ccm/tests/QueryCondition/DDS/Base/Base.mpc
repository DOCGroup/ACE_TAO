// $Id$

project(Base_stub) : taoidldefaults, ndds_ts_defaults, typecode, nddslib {
  sharedname = Base_stub
  dynamicflags += BASE_STUB_BUILD_DLL NDDS_USER_DLL_EXPORT
  libout = ../descriptors

  idlflags += -SS -Sci \
              -Wb,stub_export_macro=BASE_STUB_Export \
              -Wb,stub_export_include=Base_stub_export.h

  IDL_Files {
    idlflags += -Gxhst
    Base.idl
  }

  NDDSTypeSupport_Files {
    Base.idl
  }

  Source_Files {
  }
}

