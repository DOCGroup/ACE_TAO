project(*idl): taoidldefaults, ami {
  IDL_Files {
    ami_test.idl
  }
  custom_only = 1
}

project(*Server): messaging, taoexe, portableserver, ami {
  after += *idl
  exename = server
  macros += TEST_OUTPUT_HAS_DLL=0
  Source_Files {
    ami_testS.cpp
    ami_testC.cpp
    Test_Output.cpp
    ami_test_i.cpp
    server.cpp
  }
  IDL_Files {
  }
}

project(*Client): messaging, taoexe, portableserver, ami {
  after += *idl
  exename = client
  macros += TEST_OUTPUT_HAS_DLL=0
  Source_Files {
    ami_testS.cpp
    ami_testC.cpp
    Test_Output.cpp
    client.cpp
  }
  IDL_Files {
  }
}

project(*Simple Client): messaging, taoexe, portableserver, ami {
  after += *idl
  exename = simple_client
  macros += TEST_OUTPUT_HAS_DLL=0
  Source_Files {
    ami_testS.cpp
    ami_testC.cpp
    Test_Output.cpp
    simple_client.cpp
  }
  IDL_Files {
  }
