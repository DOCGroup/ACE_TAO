project(*Server): taoserver {
  Source_Files {
    Process_Factory.cpp
    Startup_Callback.cpp
    Server_Task.cpp
    server.cpp
  }
}

project(*Client): taoclient {
  Source_Files {
    TestC.cpp
    Client_Task.cpp
    client.cpp
  }
}

project(*Child): taoserver {
  exename = child
  Source_Files {
    Process.cpp
    child.cpp
  }
}

