#
# Makefile for building the Explicit_Event_Loop server
#

NAME = server

TAO_IDL = $(CORE_BINDIR)\tao_idl -g $(CORE_BINDIR)\gperf.exe -Ge 1

OBJFILES = \
	$(OBJDIR)\timeS.obj \
	$(OBJDIR)\timeC.obj \
	$(OBJDIR)\server.obj

CFLAGS = $(ACE_CFLAGS) $(TAO_CFLAGS)

LIBFILES = $(ACE_LIB) $(TAO_LIB)

CPPDIR = .

IDLDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

#
# IDL Build rules
#

$(IDLDIR)\timeS.cpp $(IDLDIR)\timeC.cpp: $(IDLDIR)\time.idl
	$(TAO_IDL) $**
