// -*- MPC -*-
// $Id$

project(*Server): taoserver, messaging, ami {
  exename = blocking_server
  Source_Files {
    Manager.cpp
    blocking_server.cpp
  }
}

project(*Client): messaging, taoexe, portableserver, ami {
  after += *Server
  Source_Files {
    Controller.cpp
    blocking_client.cpp
  }
}

project(*AMI_Server): taoserver, messaging, ami {
  exename = ami_server
  after += *Client
  Source_Files {
    AMI_Manager.cpp
    ami_server.cpp
  }
}

project(*AMI_Client): messaging, taoexe, portableserver, ami {
  after += *AMI_Server
  Source_Files {
    Controller.cpp
    Manager_Handler.cpp
    ami_client.cpp
  }
}

