NAME = TTest

TAO_IDL = $(CORE_BINDIR)\tao_idl -g $(CORE_BINDIR)\gperf.exe -Ge 1

OBJFILES = \
	$(OBJDIR)\TTestC.obj \
	$(OBJDIR)\TTestS.obj \
	$(OBJDIR)\TT_Info.obj \
	$(OBJDIR)\Service_Type_Exporter.obj \
	$(OBJDIR)\Offer_Exporter.obj \
	$(OBJDIR)\Offer_Importer.obj \
	$(OBJDIR)\Simple_Dynamic.obj

CFLAGS = \
	$(ACE_CFLAGS) \
	$(TAO_CFLAGS) \
	$(TAO_PORTABLESERVER_CFLAGS) \
	$(TAO_IORTABLE_CFLAGS) \
	$(TAO_DYNAMICANY_CFLAGS) \
	$(TAO_SVC_UTILS_CFLAGS) \
	$(TAO_NAMING_CFLAGS) \
	$(TAO_TRADING_CFLAGS)

LIBFILES = \
	$(ACE_LIB) \
	$(TAO_LIB) \
	$(TAO_PORTABLESERVER_LIB) \
	$(TAO_IORTABLE_LIB) \
	$(TAO_DYNAMICANY_LIB) \
	$(TAO_SVC_UTILS_LIB) \
	$(TAO_NAMING_LIB) \
	$(TAO_TRADING_LIB)

IDLFILES = \
	$(IDLDIR)\TTest.idl

CPPDIR = .

IDLDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_lib.bor>

#
# IDL Build rules
#

$(IDLDIR)\TTestS.cpp $(IDLDIR)\TTestC.cpp: $(IDLDIR)\TTest.idl
	$(TAO_IDL) $**
