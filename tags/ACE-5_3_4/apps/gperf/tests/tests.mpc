project(cinset): gperf_test {
  exename = cout

  verbatim(gnuace, bottom) {
    all: runtests

    runtests:
    "	@echo "Testing cout"
    "	./cout -v < c.gperf > cout.out
    "	-diff -b c.exp cout.out"
  }

  GPERF_Files {
    commandflags += -a -p -c -l -S1 -o
    c.gperf
  }

  Source_Files {
    c.cpp
  }
}

