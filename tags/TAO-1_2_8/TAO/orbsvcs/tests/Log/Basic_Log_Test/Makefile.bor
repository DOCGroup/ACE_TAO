	$(TAO_LIB) \
	$(TAO_PORTABLESERVER_LIB) \
	$(TAO_LOG_LIB) \
	$(TAO_NAMING_LIB) \
	$(TAO_DYNAMICANY_LIB) \
	$(TAO_SVC_UTILS_LIB)

CPPDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>
