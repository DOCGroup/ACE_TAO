project(*Cons) : orbsvcsexe, event, event_skel, rtevent, rtsched, rtevent_serv {
  after += CosEC_RtEC_Based_lib
  libs += CosEC_RtEC_Based
  libpaths += ../../lib

  specific (automake) {
    includes += $(srcdir)/../../lib
  } else {
    includes += ../../lib
  }

  source_files {
    Consumer.cpp
    Multiple.cpp
  }
}

project(*Supp) : orbsvcsexe, event, event_skel, rtevent, rtsched, rtevent_serv {
  after += CosEC_RtEC_Based_lib
  libs += CosEC_RtEC_Based
  libpaths += ../../lib

  specific (automake) {
    includes += $(srcdir)/../../lib
  } else {
    includes += ../../lib
  }

  source_files {
    Supplier.cpp
    Multiple.cpp
  }
}
