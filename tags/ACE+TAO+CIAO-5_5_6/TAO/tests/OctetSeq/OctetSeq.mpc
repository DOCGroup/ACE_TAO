// -*- MPC -*-
// $Id$

project(*idl): taoidldefaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  avoids += ace_for_tao
  after += *idl
  Source_Files {
    test_i.cpp
    server.cpp
    testS.cpp
    testC.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, anytypecode {
  avoids += ace_for_tao
  after += *idl
  Source_Files {
    testC.cpp
    client.cpp
  }
  IDL_Files {
  }
}

project(*OctetSeq): taoclient, anytypecode {
  exename = OctetSeq
  after += *idl
  Source_Files {
    testC.cpp
    OctetSeq.cpp
  }
  IDL_Files {
  }
}
