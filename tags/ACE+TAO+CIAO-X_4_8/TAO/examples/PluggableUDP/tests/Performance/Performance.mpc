// -*- MPC -*-
// $Id$

project(*server): taoserver, strategies {
  idlflags += -Sd -Sp

  Source_Files {
    UDP_i.cpp
    server.cpp
  }
}

project(*client): taoserver, strategies {
  exename = client
  idlflags += -Sd -Sp
  after += *server
  Source_Files {
    client.cpp
    UDP_i.cpp
    UDP_PerformanceClient.cpp
  }
}

