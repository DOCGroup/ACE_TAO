project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): messaging, taoexe, portableserver {
  after += *idl
  Source_Files {
    Oneway_Buffering.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): messaging, taoexe, portableserver {
  requires += corba_messaging
  after += *idl

  Source_Files {
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}

project(*Admin): messaging, taoexe, portableserver {
  after += *idl
  Source_Files {
    Oneway_Buffering_Admin.cpp
    admin.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}
