// -*- MPC -*-
// $Id$

project(Netsvcs_Dump_Restore_Lib): acelib {
  avoids       += ace_for_tao
  sharedname    = Dump_Restore
  dynamicflags += ACE_BUILD_SVC_DLL
  Source_Files {
    Dump_Restore.cpp
  }
}

project(Netsvcs_Dump_Restore) : aceexe {
  avoids  += ace_for_tao
  exename  = main
  libs    += Dump_Restore
  after   += Netsvcs_Dump_Restore_Lib
  Source_Files {
    main.cpp
  }
}
