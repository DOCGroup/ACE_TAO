project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Client): namingexe {
  exename = client
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}

project(*Server): namingexe, portableserver {
  exename = server
  after += *idl
  Source_Files {
    server.cpp
    Hello.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

