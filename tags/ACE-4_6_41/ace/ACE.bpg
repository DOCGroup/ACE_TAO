#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = ACER ACED
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

ACER: ACER.bpr
  $(MAKE)

ACED: ACED.bpr
  $(MAKE)
