CPPFLAGS += -I..
LIB	= libBase_Test.a
SHLIB	= libBase_Test.$(SOEXT)

FILES	= Baseline_Test \
	  mutex_test \
	  base_test

LSRC	= $(addsuffix .cpp,$(FILES))
LOBJ   	= $(LSRC:%.cpp=$(VDIR)%.o)
SHOBJ	= $(addsuffix .so,$(FILES))

LDLIBS 	= -lSynch_Lib

VLDLIBS = $(LDLIBS:%=%$(VAR))

BUILD = $(VLIB) $(VSHLIB) $(SHLIBA)

