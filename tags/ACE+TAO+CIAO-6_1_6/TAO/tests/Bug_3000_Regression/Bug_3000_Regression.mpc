project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, messaging {
  after += *idl
  Source_Files {
    Service.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, portableserver, messaging {
  after += *idl
  Source_Files {
    Callback.cpp
    client.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}
