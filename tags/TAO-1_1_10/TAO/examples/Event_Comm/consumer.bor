!include <$(ACE_ROOT)\include\makeinclude\build_example.bor>
  
# 
# IDL Build rules
# 
  
$(IDLDIR)\Event_CommC.cpp $(IDLDIR)\Event_CommS.cpp: $(IDLDIR)\Event_Comm.idl
