project(*Event_Sup): rtoldevent, namingexe, utils {
  exename = Event_Sup
  idlflags += -I$(TAO_ROOT)/orbsvcs/orbsvcs
  includes += $(TAO_ROOT)/orbsvcs/orbsvcs $(TAO_ROOT)/orbsvcs

  IDL_Files {
    gendir = .
    ../NavWeap.idl
  }
  Source_Files {
    Event_Sup.cpp
    DOVE_Supplier.cpp
  }
}

