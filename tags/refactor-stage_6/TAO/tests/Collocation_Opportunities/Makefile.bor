#
# Makefile for building the Collocation_Opportunities test
#

NAME = Collocation_Opportunities

TAO_IDL = $(CORE_BINDIR)\tao_idl -g $(CORE_BINDIR)\gperf.exe -Ge 1

OBJFILES = \
	$(OBJDIR)\testC.obj \
	$(OBJDIR)\testS.obj \
	$(OBJDIR)\Collocation_Opportunities.obj

CFLAGS = \
	$(ACE_CFLAGS) \
	$(TAO_CFLAGS) \
        $(TAO_PORTABLESERVER_CFLAGS) 

LIBFILES = \
	$(ACE_LIB) \
	$(TAO_LIB) \
        $(TAO_PORTABLESERVER_LIB) 

CPPDIR = .

IDLDIR = .

IDLFILES = \
        test.idl

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

#
# IDL Build rules
#

$(IDLDIR)\testC.cpp $(IDLDIR)\testS.cpp: $(IDLDIR)\test.idl
	$(TAO_IDL) $**



