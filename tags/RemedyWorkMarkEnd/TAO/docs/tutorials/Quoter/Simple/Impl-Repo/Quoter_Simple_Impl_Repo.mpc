project(*client) : taoexe, portableserver, rtevent, naming  {
  libs += Quoter_Idl_Lib
  after += Quoter_Idl_Lib

  specific (automake) {
    includes += $(srcdir)/../../idl
  }
  includes += ../../idl
  libpaths += ../../idl

  source_files {
    client.cpp
  }
}

project(*server) : taoserver, rtevent, naming, iortable {
  libs += Quoter_Idl_Lib
  after += Quoter_Idl_Lib

  specific (automake) {
    includes += $(srcdir)/../../idl
  }
  includes += ../../idl
  libpaths += ../../idl

  source_files {
    server.cpp
    Stock_Factory_i.cpp
    Stock_i.cpp
  }
}
