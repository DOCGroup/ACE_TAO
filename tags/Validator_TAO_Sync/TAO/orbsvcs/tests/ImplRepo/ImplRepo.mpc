project(airplane server) : server, orbsvcsexe, minimum_corba {
  depends += IORTable
  libs    += TAO_IORTable
  exename  = airplane_server

  IDL_Files {
    Airplane.idl
  }

  Source_Files {
    AirplaneC.cpp
    AirplaneS.cpp
    airplane_i.cpp
    airplane_server_i.cpp
    airplane_server.cpp
  }
}

