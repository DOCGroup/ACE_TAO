project(*Main): taoserver, messaging, gen_ostream {
  exename = main
  after += *DLL
  libs += IDL_Test_DLL
  verbatim(gnuace, macros) {
    ifeq ($(OSTYPE),VMS)
      CCFLAGS += -Wc/DISTINGUISH_NESTED_ENUMS
    endif
  }

  IDL_Files {
  }

  Source_Files {
    main.cpp
  }

  Header_Files {
  }

  Inline_Files {
  }

  Template_Files {
  }
}

